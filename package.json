        "setupFilesAfterEnv": [
            "<rootDir>/setupTests.js"
        ]
    },
    "dependencies": {
        "@apollo/client": "^3.3.11",
        "@apollo/react-hooks": "^4.0.0",
        "@react-native-community/async-storage": "~1.11.0",
