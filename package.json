        "@testing-library/react-native": "^7.1.0",
        "@types/react": "~16.9.41",
        "@types/react-native": "~0.62.13",
        "babel-eslint": "^10.1.0",
        "babel-preset-expo": "~8.1.0",
        "eslint": "^7.20.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-jest": "^24.1.3",
        "eslint-plugin-react": "^7.22.0",
        "jest": "^26.6.3",
